
print("half of pyramid of stars")
num1=int(input("enter the number rows "))
for i in range(num1):
    for j in range(i+1):
        print("so ",end="")
    print()
rows=int(input("enter MORE rows hahahah"))
num=1
for i in range(1,rows+1):
    for j in range(1,i+1):
        print(num,end=" ")
        num=num+1
    print()